.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.moreLine, .detail .main-content .video-recommend .video-list .items .other .title, .detail .main-content .activity-recommend .items .title {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  line-clamp: 2;
  overflow: hidden;
}

